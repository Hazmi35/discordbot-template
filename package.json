{
  "name": "discordbot-template",
  "version": "0.1.0",
  "private": true,
  "description": "A Discord bot template made in TypeScript with discord.js",
  "repository": "https://github.com/Hazmi35/discordbot-template",
  "license": "AGPL-3.0",
  "author": "Hazmi35 <contact@hzmi.xyz>",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run lint && npm run compile",
    "compile": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "node dist/index.js",
    "start:dev": "ts-node -r dotenv/config src/index.ts"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@hazmi35/eslint-config/typescript/node"
  },
  "dependencies": {
    "date-fns": "^2.22.1",
    "discord.js": "^12.5.3",
    "got": "^11.8.2",
    "tslib": "^2.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^4.0.0",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.27.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.5.3",
    "bufferutil": "^4.0.3",
    "erlpack": "github:discord/erlpack",
    "libsodium-wrappers": "^0.7.9",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=14.x"
  }
}
