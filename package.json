{
  "name": "discordbot-template",
  "version": "0.1.0",
  "private": true,
  "description": "A Discord bot template made in TypeScript with discord.js",
  "repository": "https://github.com/Hazmi35/discordbot-template",
  "license": "AGPL-3.0",
  "author": "Hazmi35 <contact@hzmi.xyz>",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn run lint && yarn run compile",
    "compile": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts --cache --cache-location .cache/eslintcache",
    "lint:fix": "eslint . --ext .ts --fix --cache --cache-location .cache/eslintcache",
    "start": "node dist/index.js",
    "start:dev": "ts-node -r dotenv/config src/index.ts"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@hazmi35/eslint-config/typescript/node"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "discord.js": "^12.4.1",
    "got": "^11.8.0",
    "tslib": "^2.0.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^2.1.3",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.3.3",
    "bufferutil": "^4.0.1",
    "erlpack": "github:discord/erlpack",
    "libsodium-wrappers": "^0.7.8",
    "utf-8-validate": "^5.0.2",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=14.x"
  }
}
